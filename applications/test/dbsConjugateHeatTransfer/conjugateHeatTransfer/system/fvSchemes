FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    default         none;;

    div(phiByEpsf,U)    Gauss linearUpwind grad(U);
    
    div(phi,h)          bounded Gauss limitedLinear 1;
    div(phi,e)          bounded Gauss limitedLinear 1;
    div(phiv,p)         bounded Gauss limitedLinear 1;
    div(phi,K)          bounded Gauss limitedLinear 1;



}

laplacianSchemes
{
    default             none;
    laplacian(thermo:mu,U)  Gauss linear orthogonal;
    laplacian(rhorAUf,p_rgh)  Gauss linear orthogonal;
    laplacian(((eps*alphahe)+((rock*kappa)|Cpv)),h) Gauss harmonic orthogonal;
    // laplacian(alphaEff,h) Gauss harmonic orthogonal;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         orthogonal;
}

