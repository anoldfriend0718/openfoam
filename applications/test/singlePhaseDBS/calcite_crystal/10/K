FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "10";
    object      K;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 0 0 0 0 0];

internalField   uniform 1e-15;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    fixedwall
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


