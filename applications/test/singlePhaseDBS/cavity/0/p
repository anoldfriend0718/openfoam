FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{

    "(left|right|bottom)"
    {
        type            zeroGradient;
    }

    movingTop
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
    
}

