FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1e-5;

boundaryField
{
    fixedWalls
    {
        type            kqRWallFunction;
        value           $internalField;
    }

    left
    {
        type            cyclic;
    }

    right
    {
        type            cyclic;
    }

    frontAndBack
    {
        type            empty;
    }
    #includeEtc "caseDicts/setConstraintTypes"
}


// ************************************************************************* //
