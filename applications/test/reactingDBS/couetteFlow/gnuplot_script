set terminal qt
#set term postscript color
set xlabel "X"
set ylabel "Ux"
set title "velocity validation"

lx=0.5
ly=0.1
eps=0.1
K=1.23457e-07
r=sqrt(eps/K)
u0=0.001

set yrange[-1e-4:u0]

plot [0:ly] u0*sinh(r*x)/sinh(r*ly) with lines lc rgb 'red' title "Analytical solution", \
	'./postProcessing/singleGraph/5/line_U.xy' with points ls 4 lc rgb 'green' title "Numerical solution"

pause -1

#    EOF
