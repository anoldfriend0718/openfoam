FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      eps;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    "(sideWalls|floor|ceiling)"
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
    
}

