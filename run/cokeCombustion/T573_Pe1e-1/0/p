FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e+06;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 1e+06;
    }
    outlet
    {
        type            calculated;
        value           uniform 1e+06;
    }
    top
    {
        type            calculated;
        value           uniform 1e+06;
    }
    bottom
    {
        type            calculated;
        value           uniform 1e+06;
    }
    frontAndBack
    {
        type            empty;
    }
}


